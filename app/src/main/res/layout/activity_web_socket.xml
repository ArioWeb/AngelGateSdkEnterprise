<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AngelsGateWebsocket.WebSocketActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/preAuth"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="preAuth Socket" />

        <Button
            android:id="@+id/postAuth"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="postAuth Socket" />

        <Button
            android:id="@+id/test"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="test Socket" />


        <Button
            android:id="@+id/signal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="signal Socket" />


        <Button
            android:id="@+id/select_file"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="selectFile Socket" />


        <TextView
            android:id="@+id/output"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:textSize="16sp" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>